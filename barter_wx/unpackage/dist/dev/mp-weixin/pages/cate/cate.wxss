/* 水平间距 */
page {
  height: 100%;
}
.select {
  color: #FF5A5F;
}
.pics {
  height: 100%;
  display: flex;
}
.pics .scroll {
  padding-bottom: 120rpx;
}
.pics .left {
  width: 200rpx;
  height: 100%;
  border-right: 1px solid #eee;
}
.pics .left view {
  height: 60px;
  line-height: 60px;
  color: #333;
  text-align: center;
  font-size: 30rpx;
  border-bottom: 1px solid #eee;
}
.pics .left .active {
  background: #d81e06;
  color: #fff;
}
.pics .right {
  text-align: center;
}
.pics .right .item image {
  width: 250rpx;
  height: 250rpx;
}
.pics .right .item .intro view:nth-child(1) {
  color: #d81e06;
}
.pics .right .isOver {
  text-align: center;
  font-size: 30rpx;
}

