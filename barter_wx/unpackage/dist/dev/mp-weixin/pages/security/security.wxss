/* 水平间距 */
.security {
  justify-content: space-between;
}
.security .item {
  display: flex;
  border-bottom: 2rpx solid #666666;
  justify-content: space-between;
  margin-bottom: 50rpx;
}
.security .item .icon-zhengque {
  color: #18BC37;
  font-size: 50rpx;
}
.security .item .icon-cuowu {
  color: #d81e06;
