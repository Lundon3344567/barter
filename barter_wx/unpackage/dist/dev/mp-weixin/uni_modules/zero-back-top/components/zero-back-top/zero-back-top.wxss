@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.zero-back-top {
  position: fixed;
  overflow: hidden;
  border-radius: 50%;
  -webkit-animation: fadeIn 0.3s linear both;
          animation: fadeIn 0.3s linear both;
  box-shadow: 0 0 10px #999999;
}
.fade-out {
  -webkit-animation: fadeOutUp 0.3s linear both;
          animation: fadeOutUp 0.3s linear both;
}
@font-face {
  font-family: iconfont;
  src: url(data:font/ttf;base64,);
}
.icon {
  font-family: iconfont;
}
@-webkit-keyframes fadeIn {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@keyframes fadeIn {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@-webkit-keyframes fadeOutUp {
0% {
    opacity: 1;
}
100% {
    opacity: 0;
    -webkit-transform: translate3d(0, -300px, 0);
    transform: translate3d(0, -300px, 0);
}
}
@keyframes fadeOutUp {
0% {
    opacity: 1;
}
100% {
    opacity: 0;
    -webkit-transform: translate3d(0, -300px, 0);
    transform: translate3d(0, -300px, 0);
}
}

